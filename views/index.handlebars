<h1>Burger Time!</h1>


<h2>Add a Burger</h2>
<div class="row">
    <div class="col-med-6 text-center"></div>
    <form class="create-form">
        <div class="form-group">
            {{#each hbsObject}}
            {{#unless devoured}}
            <lable for="addBurger">{{this.burger_name}}</lable>
            <input type="text" id="addBurger" name="name">
            {{/unless}}
            {{/each}}
        </div>
</div>
</div>
<button type="submit">Submit</button>

</form>