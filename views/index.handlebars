<h1>Burger Time!</h1>


<h2>Devour The Burger</h2>
<div class="row">
    <div class="col-med-6 text-center">
        <form class="create-form">
            <div class="form-group">
                {{#each hbsObject}}
                {{#unless devoured}}
                <lable for="devour">{{this.burger_name}}</lable>
                <input type="hidden" class="hbsObject" name="name">
                {{/unless}}
                {{/each}}
            </div>
    </div>
</div>
<button type="submit">Devour It</button>

</form>